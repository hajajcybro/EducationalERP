    </record>

       <!-- Teacher -->
    <record id="group_education_hostel_teacher" model="res.groups">
      <field name="name">Teacher</field>
      <field name="privilege_id" ref="privilege_education_hostel"/>
    </record>

    <!-- Student -->
    <record id="group_education_student" model="res.groups">
      <field name="name">Student</field>
      <field name="privilege_id" ref="privilege_education_hostel"/>
    </record>

    <!-- Parent -->
    <record id="group_education_hostel_parent" model="res.groups">
      <field name="name">Parent</field>
      <field name="privilege_id" ref="privilege_education_hostel"/>
    </record>

