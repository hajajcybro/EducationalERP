<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
<!--    Form view for education.room_member model-->
      <record id="education_room_member_view_form" model="ir.ui.view">
            <field name="name">education.room_member.view.form</field>
            <field name="model">education.room_member</field>
            <field name="arch" type="xml">
                <form string="Room Member">
                    <sheet>
                        <group col="4" name="main2">
                            <field name="room_member_rel"/>
                            <field name="hostel_room_rel"/>
                            <field name="room_member" />
                            <field name="floor"/>
                            <field name="allocated_date"/>
                            <field name="vacated_date"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
<!--    Tree view-->
        <record id="education_room_member_view_tree" model="ir.ui.view">
            <field name="name">education.room_member.view.tree</field>
            <field name="model">education.room_member</field>
            <field name="arch" type="xml">
                <tree string="Room Member">
                    <field name="room_member" />
                    <field name="allocated_date"/>
                    <field name="vacated_date"/>
                </tree>
            </field>
        </record>
</odoo>
